@model LibraryProject.Models.Entity.Tbl_Staff
@{
    ViewBag.Title = "CallStaff";
    Layout = "~/Views/Shared/Layout.cshtml";
}

@using (Html.BeginForm("UpdateStaff", "Staff", FormMethod.Post))
{
    <div style="margin-bottom:15px"></div>
    <div class="form-group">
        @Html.LabelFor(c => c.ID)
        @Html.TextBoxFor(c => c.ID, new { @class = "form-control" })
        <br />
        @*@Html.LabelFor(c => c.Staff)*@
        <label>Staff Name</label>
        @Html.TextBoxFor(c => c.Staff, new { @class = "form-control" })
    </div>
    <div>
        <button class="btn btn-warning">Update</button>
    </div>
}


